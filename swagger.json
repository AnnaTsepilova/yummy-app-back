    "components": {
        "schemas": {
            "RegistrationRequest": {
                "type": "object",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "description": "User's email",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "description": "User's password",
                        "example": "qwerty123"
                    }
                }
            },
            "RegistrationResponse": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "email": {
                            "type": "string",
                            "description": "User's email",
                            "format": "email"
                        },
                        "userId": {
                            "type": "number",
                            "description": "User's id",
                            "example": "32143232436545474"
                        }
                    }
                },
                "example": [
                    {
                        "email": "1@gmail.com",
                        "userId": "1"
                    },
                    {
                        "email": "2@gmail.com",
                        "userId": "2"
                    }
                ]
            },
            "searchRequest": {
                "type": "object",
                "required": [
                    "value",
                    "flag"
                ],
                "properties": {
                    "value": {
                        "type": "string",
                        "description": "recipe's or ingredient's name",
                        "example": "beef || Chicken"
                    },
                    "flag": {
                        "type": "string",
                        "description": "flag for search",
                        "example": "title || ingredient"
                    }
                }
            },
            "addRequest": {
                "type": "object",
                "required": [
                    "title",
                    "description",
                    "category",
                    "cockingTime",
                    "ingredients",
                    "preparation"
                ],
                "properties": {
                    "title": {
                        "type": "string",
                        "description": "recipe's name",
                        "example": "bestRecipe"
                    },
                    "description": {
                        "type": "string",
                        "description": "recipe's description",
                        "example": "chiken's roll"
                    },
                    "category": {
                        "type": "string",
                        "description": "recipe's category",
                        "example": "Breakfast"
                    },
                    "cockingTime": {
                        "type": "string",
                        "description": "coock time",
                        "example": "45min"
                    },
                    "ingredients": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "key": {
                                    "type": "string",
                                    "example": "Avocado"
                                },
                                "value": {
                                    "type": "string",
                                    "example": "1 lm"
                                }
                            }
                        },
                        "description": "ingredients",
                        "example": [
                            {
                                "avocado": "1 lim"
                            },
                            {
                                "lime": "30 ml"
                            },
                            {
                                "lemon": "40 ml"
                            }
                        ]
                    },
                    "preparation": {
                        "type": "string",
                        "description": "recipe's preparation",
                        "example": "do somthing"
                    }
                }
            },
            "addResponse": {
                "type": "string",
                "example": "success"
            },
