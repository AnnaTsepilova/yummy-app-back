        "/recipes/search": {
            "post": {
                "tags": [
                    "Recipes"
                ],
                "summary": "Search recipe by title or ingredient",
                "parameters": [
                    {
                        "in": "query",
                        "name": "page",
                        "schema": {
                            "type": "integer",
                            "minimum": 1,
                            "default": 1
